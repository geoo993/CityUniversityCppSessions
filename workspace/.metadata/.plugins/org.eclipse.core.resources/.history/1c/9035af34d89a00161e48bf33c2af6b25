#include <iostream>
using namespace std;

int main() {

	int i = 3;
	int j = 4;


	int &ref = i;
	int *pointer = j;


	cout << "Ref: " << ref << endl;
	cout << "Pointer: " << pointer << endl;
	return 0;
}
